<div class="container">
    <div class="row">
        <div class="col-md-8">
            <form method="POST" accept-charset="utf-8" action="/admin/location/edit">

                <label style="font-size:20px"> Update District Name </label>
                <input type="text" class="input-text"  name="updateLocation"  value="<%= value.targetLocation.name %>" />
                <input type="hidden" name="id" value="<%= value.targetLocation._id %>"/>

                <button class="button margin-top-20 margin-bottom-20" type="submit">
                    UPDATE</button>
            </form>
        </div>

        <%- include('./sideBar.ejs')%>

